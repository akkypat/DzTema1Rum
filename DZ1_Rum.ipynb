    "# Задание 1\n",
    "import json\n",
    "purchases  = dict()\n",
    "with open(\"purchase_log.txt\", mode = \"r\", encoding = \"utf-8\") as file:\n",
    "    file.readline()\n",
    "    for line in file.readlines():\n",
    "        data = json.loads(line)\n",
    "        purchases[data['user_id']] = data['category']\n"
   ]
  }
